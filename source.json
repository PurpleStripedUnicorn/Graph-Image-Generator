{

    "type": "parametric",

    "window": {
        "x": { "min": -10, "max": 10 },
        "y": { "min": -10, "max": 10 },
        "t": { "min": -10, "max": 10, "step": 0.002 },
        "resolution": { "x": 400, "y": 400 }
    },

    "graph": [
        {
            "x": "5 * cos(t) + cos(t*8)",
            "y": "5 * sin(t) + sin(t*8)",
            "color": [0, 255, 0],
            "dotsize": 1.3
        }
    ],

    "colors": {
        "background": [255, 255, 255],
        "axis": [150, 150, 150]
    },

    "displayformula": true

}
