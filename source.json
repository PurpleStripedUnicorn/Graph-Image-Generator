{

    "type": "parametric",

    "window": {
        "x": { "min": -5, "max": 5 },
        "y": { "min": -5, "max": 5 },
        "t": { "min": -10, "max": 10, "step": 0.002 },
        "resolution": { "x": 800, "y": 800 }
    },

    "graph": [
        {
            "x": "t",
            "y": "e ** t",
            "color": [0, 0, 255],
            "dotsize": 1.5
        },
        {
            "x": "t",
            "y": "t + 1",
            "color": [255, 0, 0],
            "dotsize": 1.5
        }
    ],

    "colors": {
        "background": [255, 255, 255],
        "axis": [150, 150, 150]
    },

    "displayformula": true

}
