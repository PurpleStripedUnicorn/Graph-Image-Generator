{

    "type": "parametric",

    "window": {
        "x": { "min": -1.7, "max": 1.7 },
        "y": { "min": -1.7, "max": 1.7 },
        "t": { "min": -10, "max": 10, "step": 0.002 },
        "resolution": { "x": 1200, "y": 1200 }
    },

    "graph": [
        {
            "x": "0.5 * cos(t) + 0.7 * cos(t*10)",
            "y": "0.5 * sin(t) + 0.7 * sin(t*10)",
            "color": [0, 0, 255],
            "dotsize": 3
        }
    ],

    "colors": {
        "background": [255, 255, 255],
        "axis": [150, 150, 150]
    },

    "displayformula": true

}
