{

    "type": "parametric",

    "window": {
        "x": { "min": -1.5, "max": 1.5 },
        "y": { "min": -1.5, "max": 1.5 },
        "t": { "min": -10, "max": 10, "step": 0.002 },
        "resolution": { "x": 400, "y": 400 }
    },

    "graph": {
        "x": "0.5 * cos(t) + 0.7 * cos(t*10)",
        "y": "0.5 * sin(t) + 0.7 * sin(t*10)"
    },

    "colors": {
        "background": [255, 255, 255],
        "line": [0, 0, 255],
        "axis": [150, 150, 150]
    },

    "dotsize": 0,
    "axissize": 0

}
